{
  "name": "fake-server",
  "version": "0.1.0",
  "description": "A fake server for testing",
  "license": "MIT",
  "keywords": [
    "testing",
    "fake-server",
    "xhr",
    "fetch"
  ],
  "author": {
    "name": "Liron Navon",
    "email": "lironavon42@gmail.com"
  },
  "scripts": {
    "start": "webpack-dev-server --env development",
    "build": "webpack --env.target production",
    "lint:js": "eslint app/**/*.js --cache",
    "test": "jest"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^22.4.3",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": "airbnb-base",
    "rules": {
      "padded-blocks": [
        2,
        "never"
      ],
      "consistent-return": [
        0,
        "never"
      ],
      "import/prefer-default-export": [
        0,
        "never"
      ],
      "no-param-reassign": [
        0,
        "never"
      ]
    }
  }
}
